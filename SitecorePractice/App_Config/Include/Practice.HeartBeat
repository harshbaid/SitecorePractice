<?xml version="1.0"?>
<configuration xmlns:patch="http://www.sitecore.net/xmlconfig/">
  <sitecore>
    <settings>
      <!--
        Exclude connection strings from Heartbeart ( http://practice.local/sitecore/service/heartbeat.aspx)
        Generally you will like to exclude LDAP, WFFM remote, CRM, MondoDB, etc connections which are non-MSSQL
      -->
      <setting name="Sitecore.Services.Heartbeat.ExcludeConnection" value="true"/>
    </settings>
  </sitecore>
</configuration>